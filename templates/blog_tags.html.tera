{% extends "base" %}
{% block content %}
    <div class="text-container">
        <h1>All the tags</h1>
        <ul>
            {% for tag in tags %}
                <li><a href="/blog/tags/{{ tag }}">{{ tag }}</a></li>
            {% endfor %}
        </ul>
        <div class="blog-footer">
            <div></div>
            <div class="blog-footer-center"><a href="#top">{% include "icons/arrow-up" %} To the top {% include "icons/arrow-up" %}</a></div>
            <div></div>
        </div>
    </div>
{% endblock content %}